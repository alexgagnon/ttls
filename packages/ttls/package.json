{
  "name": "ttls",
  "version": "1.2.1",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./html": "./dist/html/index.js",
    "./html/*.js": "./dist/html/*.js",
    "./css": "./dist/css/index.js",
    "./css/*.js": "./dist/css/*.js",
    "./xml": "./dist/xml/index.js",
    "./xml/*.js": "./dist/xml/*.js"
