  "license": "ISC",
  "description": "Tagged template literals for various languages.",
  "devDependencies": {
    "@types/cssesc": "^3.0.2",
    "@types/escape-html": "^1.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.14",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "ttls-raw": "workspace:*",
    "cssesc": "^3.0.0",
    "dompurify": "^3.2.6",
    "escape-html": "^1.0.3",
    "fast-xml-parser": "^5.2.5",
    "jsdom": "^26.1.0",
    "lightningcss": "^1.30.1"
  }
