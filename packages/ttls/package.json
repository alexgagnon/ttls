  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc"
  },
  "keywords": [
    "template-literals",
    "tagged-template-literals",
    "html",
    "css",
    "xml",
    "sql",
    "javascript",
    "typescript"
